{
    "env": {
        "browser": true,
        "es6": true,
        "amd": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "globals": {
        "M": "readonly",
        "Y": "readonly"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ]
    }
}